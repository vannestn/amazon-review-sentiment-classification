        "# Random oversampling to balance the class distribution for the binary sentiment class\n",
        "# For relevant research, see: https://aihub.org/2023/03/30/methods-for-addressing-class-imbalance-in-deep-learning-based-natural-language-processing/\n",
        "# Example usage: https://machinelearningmastery.com/random-oversampling-and-undersampling-for-imbalanced-classification/\n",
        "\n",
        "from collections import Counter\n",
        "from imblearn.over_sampling import RandomOverSampler\n",
        "from imblearn.under_sampling import RandomUnderSampler\n",
        "\n",
        "# define dataset\n",
        "X = dfc[['clean_reviews','clean_summaries']]\n",
        "y = dfc['sentiments']\n",
        "\n",
        "# Summarize class distribution before over-sampling\n",
        "print(\"Class distribution before over-sampling:\", Counter(y))\n",
        "\n",
        "# Define oversampling strategy\n",
        "oversample = RandomOverSampler(sampling_strategy='minority')\n",
        "\n",
        "# Fit and apply the transform\n",
        "X_over, y_over = oversample.fit_resample(X, y)\n",
        "\n",
        "# Summarize class distribution after over-sampling\n",
        "print(\"Class distribution after over-sampling:\", Counter(y_over))\n",
        "\n",
        "# Combine X_over (features) and y_over (target variable) into a single DataFrame\n",
        "odfc = pd.concat([X_over, y_over], axis=1)\n",
        "\n",
        "print(f'The new number of samples in our dataset ({odfc.shape[0]}) is equal to twice the \\nnumber of samples of the majority class ({Counter(y)[1]} * 2 = {Counter(y)[1]*2}), which \\nmakes sense because we oversampled our minority class to have equal \\nrepresentation of both classes in our dataset.')\n",
        "\n",
        "# Display the combined DataFrame\n",
        "odfc.head()\n"
      ]
