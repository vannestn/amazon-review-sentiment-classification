      "source": [
        "from scipy.stats import shapiro, ttest_ind, mannwhitneyu\n",
        "\n",
        "# Descriptive statistics\n",
        "grouped_stats = df.groupby('verified')['overall'].describe()\n",
        "\n",
        "# Normality test\n",
        "shapiro_verified = shapiro(df[df['verified']]['overall'])\n",
        "shapiro_unverified = shapiro(df[~df['verified']]['overall'])\n",
        "\n",
        "# Choosing and performing the statistical test based upon whether data is normally distributed\n",
        "if shapiro_verified.pvalue > 0.05 and shapiro_unverified.pvalue > 0.05:\n",
        "    # Data is approximately normally distributed, use t-test\n",
        "    u_statistic, p_value = ttest_ind(df[df['verified']]['overall'],\n",
        "                            df[~df['verified']]['overall'])\n",
        "else:\n",
        "    # Use non-parametric Mann-Whitney U test\n",
        "    u_statistic, p_value = mannwhitneyu(df[df['verified']]['overall'],\n",
        "                               df[~df['verified']]['overall'])\n",
        "\n",
        "# Calculating the effect size (rank-biserial correlation)\n",
        "n1 = df[df['verified']].shape[0]\n",
        "n2 = df[~df['verified']].shape[0]\n",
        "rbc = 1 - (2 * u_statistic) / (n1 * n2)  # Rank-biserial correlation formula\n",
        "\n",
        "print(u_statistic, p_value, rbc)"
      ]
    },
