      "source": [
        "training_review_sentences = odfc['clean_reviews'][:split]\n",
        "training_labels = odfc['sentiments'][:split]\n",
        "testing_review_sentences = odfc['clean_reviews'][split:]\n",
        "testing_labels = odfc['sentiments'][split:]\n",
        "\n",
        "review_texts = training_review_sentences.tolist()\n",
        "summary_texts = dfc['clean_summaries'].tolist()\n",
        "\n",
        "from tensorflow.keras.preprocessing.text import Tokenizer\n",
        "from tensorflow.keras.preprocessing.sequence import pad_sequences\n",
        "from tensorflow.keras.models import Model\n",
        "from tensorflow.keras.layers import Input, Embedding, LSTM, Dense, concatenate\n",
        "\n",
        "tokenizer = Tokenizer()\n",
        "tokenizer.fit_on_texts(review_texts + summary_texts)\n",
        "\n",
        "review_sequences = tokenizer.texts_to_sequences(review_texts)\n",
        "summary_sequences = tokenizer.texts_to_sequences(summary_texts)\n",
        "\n",
        "review_padded = pad_sequences(review_sequences, maxlen=max_length)\n",
        "summary_padded = pad_sequences(summary_sequences, maxlen=max_length)\n",
        "\n",
        "# Input layers\n",
        "review_input = Input(shape=(max_length,))\n",
        "summary_input = Input(shape=(max_length,))\n",
        "\n",
        "# Shared embedding layer\n",
        "shared_embedding = Embedding(input_dim=len(tokenizer.word_index) + 1, output_dim=embedding_dim)\n",
        "\n",
        "# Embedding layers for each input\n",
        "review_embedding = shared_embedding(review_input)\n",
        "summary_embedding = shared_embedding(summary_input)\n",
        "\n",
        "# LSTM layers for each input\n",
        "review_lstm = LSTM(8)(review_embedding)\n",
        "summary_lstm = LSTM(8)(summary_embedding)\n",
        "\n",
        "# Concatenate the outputs of the two branches\n",
        "concatenated = concatenate([review_lstm, summary_lstm])\n",
        "\n",
        "# Output layer\n",
        "output = Dense(1, activation='sigmoid')(concatenated)\n",
        "\n",
        "# Create model\n",
